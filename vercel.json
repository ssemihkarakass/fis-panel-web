{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/style.css",
      "dest": "/style.css",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/app.js",
      "dest": "/app.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
